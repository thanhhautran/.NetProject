                            <a asp-area="Admin" asp-controller="Home" asp-action="Admin">
                                <i class="fas fa-chart-bar"></i>Home
                            </a>
                        </li>
                        <li>
                            <a asp-area="Admin" asp-controller="Chart" asp-action="Index">
                                <i class="fas fa-chart-bar"></i>Charts
                            </a>
                        </li>
                        <li>
                            <a asp-area="Admin" asp-controller="Table" asp-action="Index">
                                <i class="fas fa-table"></i>Tables
                            </a>
                        </li>
                        <li>
                            <a asp-area="Admin" asp-controller="Form" asp-action="Index">
                                <i class="far fa-check-square"></i>Forms
                            </a>
                        </li>
                        <li>
                            <a asp-area="Admin" asp-controller="Calendar" asp-action="Index">
