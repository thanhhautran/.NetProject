                                        </div>
                                        <div class="content">
                                            <a class="js-acc-btn" href="#">@user.taikhoan</a>
                                        </div>
                                        <div class="account-dropdown js-dropdown">
                                            <div class="info clearfix">
                                                <div class="image">
                                                    <a href="#">
                                                        <img src="~/Admin/images/avatar.jpg" alt=@user.taikhoan />
                                                    </a>
                                                </div>
                                                <div class="content">
                                                    <h5 class="name">
